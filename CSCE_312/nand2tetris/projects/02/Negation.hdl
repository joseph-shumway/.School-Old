/**
 * Negate a 16-bit number, be aware that first bit is the sign-bit
 */

CHIP Negation {
    IN in[16];
    OUT out[16];

    PARTS:
   // Put you code here:
   Not16(in=in, out=inverse16);
   Inc16(in=inverse16, out=out);
   
}