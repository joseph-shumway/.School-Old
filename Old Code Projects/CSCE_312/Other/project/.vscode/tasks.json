{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "java",
			"mainClass": "Main",
			"targetPath": "${workspaceFolder}/${workspaceFolderBasename}.jar",
			"elements": [
				"${compileOutput}",
				"${dependencies}",
			],
			"problemMatcher": [],
			"label": "java: exportjar:project",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": false,
				"clear": true
			}
		}
	]
}